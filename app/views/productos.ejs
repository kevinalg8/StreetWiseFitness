<!DOCTYPE html>
<html lang="en">

<%- include('partials/head.ejs'); %>
<link rel="stylesheet" href="css/footer.css">
<link rel="stylesheet" href="css/productos.css">
<title>Productos</title>
<body>
    <!-- NavBar -->
    <%- include('partials/navbar.ejs'); %>
    <%- include('partials/nav_img.ejs'); %>
    <div id="about" class="about">
      <div class="container">
        <h3><%= seller1.nickname %></h3>
        <div class="card-group cards-wrapper">
          <% produc.forEach( function(data1) { %>
              <div class="card" style="width: 20rem">
                <img src="<%= data1.thumbnail %>" class="card-img-top" alt="...">
                <div class="card-body">
                  <h5 class="card-title"><%= data1.title %></h5>
                  <p class="card-text"><%= data1.price %></p>
                </div>
                <div class="card-footer">
                  <a href="<%= data1.permalink %>" class="btn btn-primary">Comprar</a>
                </div>
              </div>
          <% }); %>
        </div>
        <div id="about" class="about">
          <div class="container">
          <h3><%= selle2.nickname %></h3>
            <div class="card-group cards-wrapper">
              <% produ.forEach( function(data2) { %>
                  <div class="card" style="width: 20rem">
                    <img src="<%= data2.thumbnail %>" class="card-img-top" alt="...">
                    <div class="card-body">
                      <h5 class="card-title"><%= data2.title %></h5>
                      <p class="card-text"><%= data2.price %></p>
                    </div>
                    <div class="card-footer">
                      <a href="<%= data2.permalink %>" class="btn btn-primary">Comprar</a>
                    </div>
                  </div>
              <% }); %>
            </div>
        </div>
      </div>
      <div id="about" class="about">
        <div class="container">
        <h3><%= seller3.nickname %></h3>
          <div class="card-group cards-wrapper">
            <% product.forEach( function(data3) { %>
                <div class="card" style="width: 20rem">
                  <img src="<%= data3.thumbnail %>" class="card-img-top" alt="...">
                  <div class="card-body">
                    <h5 class="card-title"><%= data3.title %></h5>
                    <p class="card-text"><%= data3.price %></p>
                  </div>
                  <div class="card-footer">
                    <a href="<%= data3.permalink %>" class="btn btn-primary">Comprar</a>
                  </div>
                </div>
            <% }); %>
          </div>
      </div>
    </div>

    
    
<%- include('partials/footer.ejs'); %>


</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>

</html>
